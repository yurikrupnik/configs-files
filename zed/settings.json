{
  "context_servers": {
    "context7": {
      "command": {
        "path": "bun",
        "args": ["-y", "@upstash/context7-mcp@latest"],
        "env": null
      },
      "settings": {}
    }
  },
  "icon_theme": "Zed (Default)",
  "agent": {
    "profiles": {
      "ask": {
        "name": "Ask",
        "tools": {
          "edit_file": true,
          "delete_path": true,
          "create_directory": true,
          "contents": true,
          "diagnostics": true,
          "fetch": true,
          "list_directory": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "open": true,
          "grep": true,
          "thinking": true,
          "web_search": true,
          "copy_path": true
        },
        "enable_all_context_servers": false,
        "context_servers": {
          "mcp-server-puppeteer": {
            "tools": {
              "puppeteer_click": false,
              "puppeteer_evaluate": false
            }
          }
        }
      }
    },
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "model_parameters": [],
    "play_sound_when_agent_done": false,
    "version": "2"
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "VSCode Dark Polished"
  },
  "base_keymap": "JetBrains",
  "cursor_blink": false,
  "title_bar": {
    "show_branch_icon": true
  }
}
